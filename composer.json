    "homepage": "http://mikehaertl.github.com/phpwkhtmltopdf/",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sandeepa Rajoriya",
            "email": "sandeepa.rajoriya@cars24.com"
        }
    ],
   "require": {
        "php": ">=5.0.0",
        "cars24/php-tmpfile": "*",
        "cars24/php-shellcommand": "*"
